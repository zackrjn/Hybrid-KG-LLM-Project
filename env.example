# Copy this file to .env at the repository root and fill in the values

# Primary provider (OpenAI-compatible endpoint is supported by Cursor for multiple providers)
OPENAI_API_KEY=

# Groq for ultra-fast Claude variants (optional)
GROQ_API_KEY=

# Ollama local models (LLaVA, Mistral, etc.)
OLLAMA_BASE_URL=http://127.0.0.1:11434

# Ngrok for private tunnels to local endpoints (optional)
NGROK_AUTHTOKEN=

# Cursor Composer execution agent toggle
COMPOSER_ENABLED=true


